[require]
GL >= 4.5
GLSL >= 4.50

[compute shader]
#version 450

layout(local_size_x = 1, local_size_y = 1, local_size_z = 1) in;
layout(std430, binding = 0) buffer buffer_0 {
 uint ext_0; // 1
 uint ext_1; // 5
};

void main()
{
 if(1u + ext_0 <= 0u)
  {
   ext_1 = 1u;
  }
}

[test]
ssbo 0 12

ssbo 0 subdata int 0 1
ssbo 0 subdata int 4 5

compute 1 1 1

probe ssbo int 0 0 == 1
probe ssbo int 0 4 == 5 
